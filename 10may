lobal main


extern printf

section .bss

var_write: resq 1

section .data

format: db "%lld", 0


section .text

main:



   push rbp
   mov rbp,rsp
   mov qword [rbp],5
   mov qword [rbp-8],13
   mov rdx, qword [rbp]
   mov rax, qword [rbp-8]
   add rax,rdx
   mov qword [var_write],rax
   pop rbp

   push rbp
   mov rdi, format
   mov rsi, [var_write]
   mov rax, 0 
   call printf
   pop rbp

   ret
